    "#Reverse the number\r\n",
    "def ReverseNumber(x):\r\n",
    "    res=0\r\n",
    "    while(x):\r\n",
    "        n=x%10\r\n",
    "        res=res*10+n\r\n",
    "        x=x//10\r\n",
    "     \r\n",
    "    return res \r\n",
    "\r\n",
    "print(ReverseNumber(int(input())))"
