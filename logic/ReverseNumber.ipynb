    "    res=0\r\n",
    "    while num:\r\n",
    "        n=num%10  #1254 --> 4\r\n",
    "        res=res*10+n #---> 0*10+4=4 | 4*10+5=45\r\n",
    "        num=num//10    #---> 1254//10=125    \r\n",
    "    \r\n",
    "    return res\r\n",
    "\r\n",
    "print(reverse(int(input())))"
